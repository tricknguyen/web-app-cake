@section Scripts { 
    <script>
        function ShowCoupon() {
            var a = @Html.Raw(Json.Serialize(ViewBag.Rank));
            var b = @Html.Raw(Json.Serialize(ViewBag.Code));
            if (a == 1) {
                Swal.fire({
                    title: 'You get 1 discount code from the shop: ' + b,
                    showClass: {
                        popup: 'animate__animated animate__fadeInDown'
                    },
                    hideClass: {
                        popup: 'animate__animated animate__fadeOutUp'
                    }
                })
                return false;
            }
            else {
                Swal.fire('Nothing')
                return false;
            }
        }
    </script>

}
