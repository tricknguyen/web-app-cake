                            </svg>

                            <br /><p>Not Orders Yet</p>
                        </div>
                    }
                    else
                    {
                        foreach (var order in Model.OrderHList)
                        {
                            if (order.OrderStatus == "Pending" || order.OrderStatus == "Approved")
                            {

                                <div class="card-order-infor">
                                    <h4><i class="fas fa-box-open"></i> <span>In Pending</span></h4>
                                    @foreach (var orderdetail in Model.OrderDList)
                                    {
                                        @if (orderdetail.OrderHeaderId == order.Id)
                                        {
                                            <div class="card-order-infor-child">
                                                <img src="@WC.ImagePath@orderdetail.Product.Image" alt="cake" />
                                                
                                                <div class="card-order-infor__text">
                                                    <h1>@orderdetail.Product.Name</h1>
                                                    <div class="card-order-infor__text__detail">
                                                        <div class="card-order-infor__text__detail__">
                                                            <p>Date Order: @order.OrderDate.Date.ToString("dd / MM / yyyy")</p>
                                                            <p>Delivery Address: @order.Address</p>
                                                            <p>Amount: @orderdetail.Sqft</p>
                                                        </div>
                                                        <div class="card-order-infor__text__total">                                                    
                                                            <p>$@(orderdetail.PricePerSqFt * orderdetail.Sqft)</p>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        }
                                    }
                    
                                    <h6>Total: $@order.FinalOrderTotal</h6>
                                </div>                                                                    
                            }
                        }
                    }
                </section>
                <section style="padding-top: 50px!important;">
                    <h2>In Processing</h2>
                    @{ var test1 = 0;}
                    @foreach (var order in Model.OrderHList)
                    {
                        if (order.OrderStatus == "Processing")
                        {
                            <div class="card-order-infor">
                                <h4><i class="fas fa-box-open"></i> <span>In Processing</span></h4>

                                @foreach (var orderdetail in Model.OrderDList)
                                {
                                    @if (orderdetail.OrderHeaderId == order.Id)
                                    {
                                        
                                        <div class="card-order-infor-child">
                                                <img src="@WC.ImagePath@orderdetail.Product.Image" alt="cake" />
                                                
                                                <div class="card-order-infor__text">
                                                    <h1>@orderdetail.Product.Name</h1>
                                                    <div class="card-order-infor__text__detail">
                                                        <div class="card-order-infor__text__detail__">
                                                            <p>Date Order: @order.OrderDate.Date.ToString("dd / MM / yyyy")</p>
                                                            <p>Delivery Address: @order.Address</p>
                                                            <p>Amount: @orderdetail.Sqft</p>
                                                        </div>
                                                        <div class="card-order-infor__text__total">                                                    
                                                            <p>$@(orderdetail.PricePerSqFt * orderdetail.Sqft)</p>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                    }
                                }
                                <h6>Total: $@order.FinalOrderTotal</h6>
                            </div>
                        }
                        else
                        {
                            test1 += 1;
                        }
                    }
                    @if(test1 == Model.OrderHList.Count())
                    {
                        <div class="card-order-infor">

